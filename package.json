{
  "name": "flywheel-mile",
  "version": "1.0.0",
  "description": "flywheel-mile",
  "private": true,
  "main": "./bin/server.js",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "bcrypt": "^0.8.7",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.6",
    "bootstrap-material-design": "^0.5.10",
    "cookie-parser": "~1.3.5",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "font-awesome": "^4.6.3",
    "morgan": "~1.6.1",
    "node-fetch": "^1.5.3",
    "node-sass": "^3.8.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sequelize": "^3.23.4",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "debug": "~2.2.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "sqlite": "0.0.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node ./build/bin/server.js",
    "test": "NODE_ENV=test node_modules/.bin/mocha test --recursive --compilers js:babel-core/register",
    "dev": "node_modules/.bin/babel-node ./bin/server.js",
    "clean": "npm run clean-server && npm run clean-client",
    "clean-server": "rm -rf build && mkdir build",
    "clean-client": "rm -rf public/dist && mkdir public/dist",
    "build": "npm run build-client && npm run build-server",
    "build-css": "node_modules/.bin/node-sass public/scss/application.scss public/dist/application.css",
    "build-client": "npm run clean-client && node_modules/.bin/webpack --config webpack.config.prod.js && npm run build-css",
    "build-server": "npm run clean-server && node_modules/.bin/babel ./ -d ./build -s --copy-files --ignore node_modules/,test/,seeders/,migrations/,build/",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:drop": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:reset": "npm run db:drop && npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supaflyqtpie/flywheel-mile.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supaflyqtpie/flywheel-mile/issues"
  },
  "homepage": "https://github.com/supaflyqtpie/flywheel-mile"
}
